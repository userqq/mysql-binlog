{
    "name": "userqq/mysql-binlog",
    "type": "project",
    "config": {
        "platform": {
            "php": "8.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-gmp": "*",
        "ext-pcntl": "*",
        "amphp/amp": "^3",
        "amphp/file": "^3",
        "amphp/log": "^2",
        "amphp/socket": "^2",
        "monolog/monolog": "^3",
        "revolt/event-loop": "^0"
    },
    "autoload": {
        "psr-4": {
            "UserQQ\\MySQL\\Binlog\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.1",
        "psalm/phar": "dev-master"
    }
}
